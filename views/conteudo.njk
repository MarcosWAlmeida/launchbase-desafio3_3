{% extends "layout.njk" %}

{%block head%}
    <title>RocketSeat - Conte√∫do </title>
{%endblock%}

{% block content%}
    <div class="cursos">
        {% for item in items %}
        <div class="curso" id="{{item.id}}">
            <img src="{{item.url_img}}" alt="starter">
            <p>{{item.modules}}</p>
            <h5>{{item.price}}</h5>
          
        </div>
        {% endfor %}  
    </div>
    <div class="modal_Overlay">
        <div class="modal">
            <iframe src="" frameborder="0"></iframe>
            <div class="close-modal">
                <span class="material-icons"> close </span>
            </div>
            <div class="maximize-modal">
                <span class="material-icons"> open_in_full </span>
            </div>
           
        </div>
    </div>
<script src="/script.js"></script>
{% endblock %}